extends ../__layout.pug

block title
  title Playback Rate

block link
  link(rel="stylesheet", href="../../css/main.css")
  link(rel="stylesheet", href="../../libs/font-awesome.min.css")


block body
  body(class="playback-body")
    header(class="playback-header")
      span Power Management
      span(class="fa fa-window-close playback-close")
      
      
      
    section
      ul(class="playback-options")
        li(class="playback-item" data-fire="playback-veryslow")
          p Very Slow
          input(type="number" min="0.1" max="50" step=".1")
        li(class="playback-item" data-fire="playback-slow")
          p Slow
          input(type="number" min="0.1" max="50" step="0.1")
        li(class="playback-item" data-fire="playback-veryfast")
          p Very Fast
          input(type="number" min="0.1" max="50" step="0.1")
        li(class="playback-item" data-fire="playback-fast")
          p Fast
          input(type="number" min="0.1" max="50" step="0.1")
    
      div(class="playback-btn")
        button(class="playback-reset") Reset
        button(class="playback-submit") Submit
    script(src="../../js/Settings/Playbackrate.js")